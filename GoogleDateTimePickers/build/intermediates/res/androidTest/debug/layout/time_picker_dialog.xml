<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/time_picker_dialog"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:background="@color/circle_background"
    android:focusable="true"
    android:orientation="vertical">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white">

        <include
            layout="@layout/time_header_label"
            android:layout_width="match_parent"
            android:layout_height="@dimen/header_height"
            android:layout_gravity="center" />
    </FrameLayout>

    <mirko.android.datetimepicker.time.RadialPickerLayout
        android:id="@+id/time_picker"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/picker_dimen"
        android:layout_gravity="center"
        android:focusable="true"
        android:focusableInTouchMode="true" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dip"
        android:background="@color/line_background" />

    <LinearLayout
        style="?android:attr/buttonBarStyle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <Button
            android:id="@+id/done_button"
            style="?android:attr/buttonBarButtonStyle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="48dp"
            android:text="@string/done_label"
            android:textColor="@color/done_text_color"
            android:textSize="@dimen/done_label_size" />
    </LinearLayout>
</LinearLayout><!-- From: file:/F:/Demo/Android/ImageTools/ImageProcesTools/GoogleDateTimePickers/res/layout/time_picker_dialog.xml --><!-- From: file:/F:/Demo/Android/ImageTools/ImageProcesTools/GoogleDateTimePickers/build/intermediates/bundles/debug/res/layout/time_picker_dialog.xml -->